<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="66dp"
    android:orientation="horizontal"
    android:background="#ffffff"
    >


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="66dp"
        android:id="@+id/delete_one_layout"
        android:gravity="right"
        android:orientation="horizontal"
        >
        <LinearLayout
            android:layout_width="66dp"
            android:layout_height="66dp"
            android:background="@color/delete_account"
            android:gravity="center">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/shangchu_liebiao"
            android:padding="8.5dp"
            />
        </LinearLayout>
    </LinearLayout>

    <com.himawari.permissionUtils.views.AboveItemView
        android:layout_width="match_parent"
        android:layout_height="66dp"
        android:id="@+id/above_item"></com.himawari.permissionUtils.views.AboveItemView>

</FrameLayout>
<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="itemview">
        <attr name="img_icon" format="reference|color"/>
        <attr name="item_title_tv" format="string"/>
        <attr name="next_icon" format="reference|color"/>
        <attr name="item_title_size" format="float"></attr>
        <attr name="item_title_color" format="color"></attr>
        <attr name="left_checkbox_button" format="reference"/>
    </declare-styleable>

    <declare-styleable name="titleview">
        <attr name="title_tv" format="string"/>
        <attr name="left_img" format="reference|color"/>
        <attr name="right_img" format="reference|color"/>
        <attr name="right_tv" format="string"/>
        <attr name="right_tv_color" format="color"/>
        <attr name="right_tv_size" format="float"/>
    </declare-styleable>

    <declare-styleable name="edittextview">
        <attr name="edit_goal_title" format="string"/>
        <attr name="edit_goal_hint" format="string"/>
        <attr name="title_margin_left" format="integer|float"/>
        <attr name="hint_margin_left" format="integer|float"/>
    </declare-styleable>

    <declare-styleable name="custommenu">
        <attr name="img1" format="reference|color"/>
        <attr name="img2" format="reference|color"/>
        <attr name="img3" format="reference|color"/>
        <attr name="img4" format="reference|color"/>
        <attr name="img5" format="reference|color"/>
        <attr name="img6" format="reference|color"/>
        <attr name="img7" format="reference|color"/>
        <attr name="img8" format="reference|color"/>
        <attr name="img9" format="reference|color"/>
        <attr name="position" format="integer"></attr>
        <attr name="distance" format="integer"></attr>
    </declare-styleable>


    <declare-styleable name="moireview">
        <attr name="moirecolor" format="color"/>
        <attr name="isStroke" format="boolean"/>
        <attr name="duration" format="integer"/>
    </declare-styleable>
</resources>